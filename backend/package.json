{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=debug nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=debug jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "cross-var cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:log": "npm run typeorm -- schema:log -d ./src/ormconfig.ts",
    "typeorm:query": "npm run typeorm -- query -d ./src/ormconfig.ts \"$npm_config_query\"",
    "typeorm:version": "npm run typeorm -- version",
    "migration:show": "npm run typeorm -- migration:show -d ./src/ormconfig.ts",
    "migration:run": "npm run build && npm run typeorm -- migration:run -d ./src/ormconfig.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/ormconfig.ts",
    "migration:generate": "npm run typeorm -- migration:generate -d ./src/ormconfig.ts ./src/migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "prestart:dev": "patch-package"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.2.4",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/typeorm": "^10.0.0",
    "@node-rs/argon2": "^1.5.2",
    "@types/passport-jwt": "^3.0.9",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.2",
    "handlebars": "^4.7.6",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "nest-winston": "^1.9.4",
    "nodemailer": "^6.4.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "patch-package": "^8.0.0",
    "pug": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@appicanis/cross-var": "^1.1.1",
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.4",
    "@types/ejs": "^3.0.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.2.6",
    "@types/ms": "^0.7.32",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.19",
    "@types/nodemailer": "^6.4.6",
    "@types/pug": "2.0.6",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "inquirer": "^9.2.10",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.6",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "@nestjs-modules/mailer/glob": "^7.1.3",
    "@nestjs-modules/mailer/**/degenerator": "^5.0.1",
    "@nestjs-modules/mailer/**/display-notification": "2.0.0",
    "@nestjs-modules/mailer/**/execa": "4.0.2",
    "@nestjs-modules/mailer/**/html-minifier": "^4.0.0",
    "@nestjs/cli/**/memfs": "^4.2.1",
    "@nestjs/cli/**/tslib": "^2.6.2",
    "@nestjs/cli/**/quill-delta": "^5.1.0",
    "semver": "^7.5.4",
    "ts-loader/webpack": "^5.74.0"
  },
  "overrides": {
    "proxy-agent": "^6.3.1",
    "semver": "^7.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
